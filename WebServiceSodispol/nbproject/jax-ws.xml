<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="WebServiceSodispol">
      <implementation-class>com.WebServiceSodispol.WebServiceSodispol</implementation-class>
    </service>
  </services>
  <clients>
    <client name="wsSODIS">
      <wsdl-url>https://ws.espol.edu.ec/saac/wsSODIS.asmx?WSDL</wsdl-url>
      <local-wsdl-file>ws.espol.edu.ec/saac/wsSODIS.asmx.wsdl</local-wsdl-file>
      <package-name>ec.edu.espol.academico.webservices</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <binding>
        <file-name>customization.xjb</file-name>
      </binding>
      <binding>
        <file-name>XMLSchema.xsd</file-name>
      </binding>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>https://ws.espol.edu.ec/saac/wsSODIS.asmx?WSDL</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
  <jsr109>false</jsr109>
</jax-ws>
